{
  "name": "api",
  "description": "Api for episodehunter",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "compile": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=develop node --inspect dist/index.js"
  },
  "author": "tjoskar <kontakta@oskarkarlsson.nu>",
  "bugs": {
    "url": "https://github.com/episodehunter/api/issues",
    "email": "kontakta@oskarkarlsson.nu"
  },
  "dependencies": {
    "apollo-engine": "^1.0.1",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "jwks-rsa": "^1.2.1",
    "knex": "^0.14.4",
    "mysql": "^2.15.0",
    "raven": "^2.4.2"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/graphql": "0.13.1",
    "@types/jest": "22.2.3",
    "@types/knex": "0.14.8",
    "@types/node": "9.6.18",
    "@types/ramda": "0.25.29",
    "@types/raven": "2.5.1",
    "jest": "22.4.2",
    "ts-jest": "22.4.1",
    "typescript": "2.7.2"
  },
  "homepage": "https://github.com/episodehunter/api",
  "keywords": [
    "api",
    "episodehunter"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/episodehunter/api"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/config.ts",
      "<rootDir>/database.ts",
      "<rootDir>/engine.ts",
      "<rootDir>/index.ts",
      "<rootDir>/root-resolver.ts",
      "<rootDir>/root-schema.ts",
      "<rootDir>/root-type.ts"
    ]
  }
}
