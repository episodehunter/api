{
  "name": "api",
  "description": "Api for episodehunter",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "compile": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=develop node --inspect dist/index.js"
  },
  "author": "tjoskar <kontakta@oskarkarlsson.nu>",
  "bugs": {
    "url": "https://github.com/episodehunter/api/issues",
    "email": "kontakta@oskarkarlsson.nu"
  },
  "dependencies": {
    "apollo-engine": "^1.0.1",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "knex": "^0.14.4",
    "mysql": "^2.15.0",
    "raven": "^2.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.12.4",
    "@types/jest": "*",
    "@types/knex": "^0.14.8",
    "@types/node": "^9.4.7",
    "@types/ramda": "^0.25.20",
    "@types/raven": "^2.1.6",
    "jest": "*",
    "ts-jest": "*",
    "typescript": "*"
  },
  "homepage": "https://github.com/episodehunter/api",
  "keywords": [
    "api",
    "episodehunter"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/episodehunter/api"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/config.ts",
      "<rootDir>/database.ts",
      "<rootDir>/engine.ts",
      "<rootDir>/index.ts",
      "<rootDir>/root-resolver.ts",
      "<rootDir>/root-schema.ts",
      "<rootDir>/root-type.ts"
    ]
  }
}
